clear
xdel(winsid())
PP=fscanfMat("pressuredata.txt");
HH=fscanfMat("deformationdata.txt");
H0=fscanfMat("initialdeform.txt");
figure(2)
subplot(1,2,1)
mesh(PP)
subplot(1,2,2)
mesh(HH)
M=size(HH);
x=linspace(0,M(1),M(1));
y=linspace(0,M(2),M(2));
figure(3)
contour(x,y,HH,10)
figure(4)
contour(x,y,PP,10)
figure(6)
mesh(H0)
